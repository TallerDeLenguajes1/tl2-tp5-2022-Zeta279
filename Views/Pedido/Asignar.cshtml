@{
    ViewData["Title"] = "Asignar pedido";
}
@model PedidoModel

<section>
    <form method="post">
        <input type="number" id="idp" name="idp" value="@Model.Nro" hidden>
        <select id="idc" name="idc">
            @foreach(var cadete in DataModel.CadeteList)
            {
                <option value="@cadete.Key">@cadete.Value.nombre</option>
            }
        </select>
        <input type="submit" value="Asignar al pedido">
    </form>
</section>